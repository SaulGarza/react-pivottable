{
  "rules": {
    "whitespace": [false],
    "variable-name": [
      false
    ],
    "ban": [
      true,
      {"name": "$", "message": "please don't"}
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  }
}